onse data\r\n        console.log(response?.accessToken);\r\n        console.log(JSON.stringify(response))\r\n        setSuccess(true);\r\n        setPass('');\r\n        setMatchPass('');\r\n        // send_reset_email(email, reset_token);\r\n\r\n        } catch (err) {\r\n            if (!err?.response) {\r\n                setErrMsg('No Server Response');\r\n            } else if (err.response?.status === 404) {\r\n                //conflict between the requested resources and the current state of the server \r\n                setErrMsg('Email not registered. Please enter another email.');\r\n            } else {\r\n                setErrMsg('Submit Failed')\r\n            }\r\n            \r\n        }\r\n    };\r\n\r\n  return (\r\n    <div id=\"loginregis\">\r\n        {success ? (\r\n                <section>\r\n                   \r\n{/*====================password field=================*/}\r\n                        <section id=\"lgsec\">\r\n                        <h1>Reset Password</h1>\r\n                        <label htmlFor=\"password\">\r\n                            Password:\r\n                            <FontAwesomeIcon icon={faCheck} className={validPass ? \"valid\" : \"hide\"} />\r\n                            <FontAwesomeIcon icon={faTimes} className={validPass || !pass ? \"hide\" : \"invalid\"} />\r\n                        </label>\r\n                        <input\r\n                            type=\"password\"\r\n                            id=\"password\"\r\n                            onChange={(e) => setPass(e.target.value)}\r\n                            value={pass}\r\n                            required\r\n                            aria-invalid={validPass ? \"false\" : \"true\"}\r\n                            aria-describedby=\"passnote\"\r\n                            onFocus={() => setPassFocus(true)}\r\n                            onBlur={() => setPassFocus(false)}\r\n                        />\r\n                        <p id=\"passdnote\" className={passFocus && !validPass ? \"instructions\" : \"offscreen\"}>\r\n                        \r\n                            <FontAwesomeIcon icon={faInfoCircle} />\r\n                            8 to 24 characters.<br />\r\n                            Must include uppercase and lowercase letters, a number and a special character.<br />\r\n                            Allowed special characters: <span aria-label=\"exclamation mark\">!</span> <span aria-label=\"at symbol\">@</span> <span aria-label=\"hashtag\">#</span> <span aria-label=\"dollar sign\">$</span> <span aria-label=\"percent\">%</span>\r\n                        </p>\r\n\r\n{/*====================confirm pass==================*/}\r\n                        <label htmlFor=\"confirm_pass\">\r\n                            Confirm Password:\r\n                            <FontAwesomeIcon icon={faCheck} className={validMatch && matchPass ? \"valid\" : \"hide\"} />\r\n                            <FontAwesomeIcon icon={faTimes} className={validMatch || !matchPass ? \"hide\" : \"invalid\"} />\r\n                        </label>\r\n                        <input\r\n                            type=\"password\"\r\n                            id=\"confirm_pass\"\r\n                            onChange={(e) => setMatchPass(e.target.value)}\r\n                            value={matchPass}\r\n                            required\r\n                            aria-invalid={validMatch ? \"false\" : \"true\"}\r\n                            aria-describedby=\"confirmnote\"\r\n                            onFocus={() => setMatchFocus(true)}\r\n                            onBlur={() => setMatchFocus(false)}\r\n                        />\r\n                        <p id=\"confirmnote\" className={matchFocus && !validMatch ? \"instructions\" : \"offscreen\"}>\r\n                            <FontAwesomeIcon icon={faInfoCircle} />\r\n                            Must match the first password input field.\r\n                        </p>\r\n                        <button onClick={handleButtonClick}>Reset Now</button>\r\n                        </section>\r\n                </section>\r\n            ) : (\r\n        <section id=\"forgotpass\">\r\n            <p ref={errRef} className={errMsg ? \"errmsg\" : \"offscreen\"} aria-live=\"assertive\">{errMsg}</p>\r\n            <h1>Forgot Password</h1>\r\n            <br/>\r\n            <form onSubmit={handleSubmit}>\r\n            <input\r\n                type=\"email\"\r\n                value={email}\r\n                onChange={(e) => setEmail(e.target.value)}\r\n                placeholder=\"Enter registered email\"\r\n        />\r\n            <button type=\"submit\" onClick={handleSubmit}>Submit</button>\r\n        </form>\r\n        <p>{message}</p>\r\n      </section>\r\n      )}\r\n    </div>\r\n  );\r\n};\r\n\r\nexport default ForgotPassword;","/Users/rana/Downloads/noteManagementSystem-main/frontend/src/components/Layout.js",[],[],"/Users/rana/Downloads/noteManagementSystem-main/frontend/src/components/RequireAuth.js",[],[],"/Users/rana/Downloads/noteManagementSystem-main/frontend/src/components/Home.js",["153","154"],[],"/Users/rana/Downloads/noteManagementSystem-main/frontend/src/Note/createNote.js",[],[],"/Users/rana/Downloads/noteManagementSystem-main/frontend/src/Note/noteBook.js",[],[],"/Users/rana/Downloads/noteManagementSystem-main/frontend/src/Note/note.js",["155","156"],[],"/Users/rana/Downloads/noteManagementSystem-main/frontend/src/api/axios.js",[],[],"/Users/rana/Downloads/noteManagementSystem-main/frontend/src/hooks/useAuth.js",[],[],{"ruleId":"157","severity":1,"message":"158","line":2,"column":10,"nodeType":"159","messageId":"160","endLine":2,"endColumn":16},{"ruleId":"157","severity":1,"message":"161","line":2,"column":32,"nodeType":"159","messageId":"160","endLine":2,"endColumn":45},{"ruleId":"157","severity":1,"message":"162","line":8,"column":8,"nodeType":"159","messageId":"160","endLine":8,"end